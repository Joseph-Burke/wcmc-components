{
  "name": "wcmc-components",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "build": "vue-cli-service build",
    "build-lib": "vue-cli-service build --target lib --name wcmc-components src/main.js",
    "lint": "vue-cli-service lint"
  },
  "main": "./dist/wcmc-components.common.js",
  "files": [
